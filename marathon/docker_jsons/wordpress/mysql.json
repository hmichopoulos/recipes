{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "d.r.cc/mysql:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 3306, "hostPort": 0 }
      ]
    }
  },
  "id": "wp-mysql2",
  "instances": 1,
  "cpus": 0.3,
  "mem": 800,
  "env": {
    "MYSQL_ROOT_PASSWORD": "xxx"
  }
}
